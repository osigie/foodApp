  "bugs": {
    "url": "https://github.com/osigie/foodApp/issues"
  },
  "homepage": "https://github.com/osigie/foodApp#readme",
  "dependencies": {
    "concurrently": "^7.2.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.16"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.42",
    "typescript": "^4.7.3"
  }
}
