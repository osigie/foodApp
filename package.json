{
  "name": "foodapp",
  "version": "1.0.0",
  "description": "minimal e-commerce",
  "main": "server.ts",
  "scripts": {
    "build":"tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "nodemon ./dist/server --ignore client",
    "client": "npm start --prefix client",
    "start": "concurrently --kill-others-on-fail \" npm run server\" \" npm run client\" \" npm run build\""
  },
